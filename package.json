{
  "name": "gulp-locales-bundler",
  "version": "0.1.1",
  "description": "Gulp plugin to bundle JSON localization files",
  "main": "./src/gulpLocalesBundler.js",
  "scripts": {
    "cover": "istanbul cover ./node_modules/mocha/bin/_mocha",
    "coveralls": "istanbul cover ./node_modules/mocha/bin/_mocha --report lcovonly -- -R spec && cat ./coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js && rm -rf ./coverage",
    "lint": "eslint ./src/ ./test/",
    "test": "mocha"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/1oginov/gulp-locales-bundler.git"
  },
  "keywords": [
    "angular-translate",
    "bundler",
    "gulp",
    "gulp-plugin",
    "json",
    "internationalization",
    "i18n",
    "l10n",
    "locales",
    "localization"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/1oginov/gulp-locales-bundler/issues"
  },
  "homepage": "https://github.com/1oginov/gulp-locales-bundler",
  "dependencies": {
    "deep-assign": "^2.0.0",
    "object-path": "^0.11.4",
    "through2": "^2.0.3",
    "vinyl": "^2.1.0"
  },
  "devDependencies": {
    "chai": "^4.1.2",
    "chai-things": "^0.2.0",
    "coveralls": "^3.0.0",
    "eslint": "^4.16.0",
    "eslint-config-google": "^0.9.1",
    "istanbul": "^0.4.5",
    "mocha": "^5.0.0"
  }
}
