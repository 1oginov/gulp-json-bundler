{
  "name": "gulp-locales-bundler",
  "version": "1.0.5",
  "description": "Gulp plugin to bundle JSON localization files",
  "main": "src/gulpLocalesBundler.js",
  "scripts": {
    "ci": "npm run lint && npm run test:coverage",
    "clean:coverage": "rm -rf coverage",
    "coveralls": "npm run test:coverage -- --report lcovonly -- -R spec && cat ./coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js && npm run clean:coverage",
    "lint": "eslint src test",
    "test": "mocha",
    "test:coverage": "istanbul cover ./node_modules/mocha/bin/_mocha"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/loginov-rocks/gulp-locales-bundler.git"
  },
  "keywords": [
    "angular-translate",
    "bundler",
    "gulp",
    "gulp-plugin",
    "i18n",
    "internationalization",
    "json",
    "l10n",
    "locales",
    "localization"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/loginov-rocks/gulp-locales-bundler/issues"
  },
  "homepage": "https://github.com/loginov-rocks/gulp-locales-bundler",
  "dependencies": {
    "deep-assign": "^3.0.0",
    "object-path": "^0.11.4",
    "through2": "^3.0.1",
    "vinyl": "^2.2.0"
  },
  "devDependencies": {
    "chai": "^4.2.0",
    "chai-things": "^0.2.0",
    "coveralls": "^3.0.4",
    "eslint": "^5.16.0",
    "eslint-config-google": "^0.13.0",
    "eslint-plugin-jsdoc": "^8.1.0",
    "istanbul": "^0.4.5",
    "mocha": "^6.1.4"
  }
}
